<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UTN\Bundle\UsuarioBundle\Entity\Transferencia" table="Transferencia">
    <id name="idTransferencia" type="integer" column="id_transferencia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idInventario" type="integer" column="id_inventario" nullable="false"/>
    <field name="idResponsableAnterior" type="integer" column="id_responsable_anterior" nullable="false"/>
    <field name="idResponsableNuevo" type="integer" column="id_responsable_nuevo" nullable="true"/>
    <field name="aprobado" type="smallint" column="aprobado" nullable="true"/>
    <field name="fechaSolicitud" type="datetime" column="fecha_solicitud" nullable="true"/>
    <field name="fechaFin" type="datetime" column="fecha_fin" nullable="true"/>
  </entity>
</doctrine-mapping>

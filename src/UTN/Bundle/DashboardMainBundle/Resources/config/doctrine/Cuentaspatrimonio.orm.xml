<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UTN\Bundle\DashboardMainBundle\Entity\Cuentaspatrimonio" table="CuentasPatrimonio">
    <id name="ctapatrimonio" type="smallint" column="ctapatrimonio">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="codgrupocuenta" type="smallint" column="codgrupocuenta" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codsubgrupoctapat" type="smallint" column="codsubgrupoctapat" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UTN\Bundle\DashboardMainBundle\Entity\Areaspatrimonio" table="AreasPatrimonio">
    <id name="area" type="smallint" column="AREA">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="NOMBRE" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="legajopersonal" type="integer" column="LEGAJOPERSONAL" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lugar" type="string" column="LUGAR" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="oper" type="string" column="__OPER" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="usuario" type="string" column="__USUARIO" length="12" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="tiempo" type="datetime" column="__TIEMPO" nullable="true"/>
  </entity>
</doctrine-mapping>
